package normal; 

import java.io.*; 

importjava.net.URL;

import java.net.MalformedURLException;

publicclassMain {

publicstaticvoidDownloadWebPage(Stringwebpage)

{

try {

URLurl=new URL(webpage);

BufferedReaderreadr=newBufferedReader(newInputStreamReader(url.openStream())); 

BufferedWriter writer = new BufferedWriter(new FileWriter("Data.html"));

String line;

while((line=readr.readLine())!=null)

{

writer.write(line);

}

readr.close(); 

writer.close();

System.out.println("THISPROGRAMISEXECUTEDBYSURESH S221211101143");

System.out.println("SuccessfullyDownloaded.");

}

catch(MalformedURLExceptionmue)

{

System.out.println("MalformedURLException raised");

}

catch(IOExceptionie)

{

System.out.println("IOExceptionraised");

}

}

publicstaticvoidmain(Stringargs[]) throws 

IOException

{

Stringurl="https://www.drmgrdu.ac.in/contact.php";DownloadWebPage(url

);

}

}
